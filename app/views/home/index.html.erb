<h1>トップページ</h1>
<button id = "find-me" onclick="location.href='/maps'" class="btn btn-success">探す！</button>
<% content_for :js do %>
  <%= javascript_import_module_tag "app/javascript/custom/geolocation" %>
<% end %>
<div id='map'></div>
<style>
#map {
  height: 600px;
  width: 600px;
}
</style>
<script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['GOOGLE_MAP_API_KEY'] %>&callback=initMap" async defer></script>
